   "outputs": [],
   "source": [
    "_, idx = np.unique(arr, return_index=True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "id": "5786340a-4a81-43a9-9d12-d1eac3e233ef",
   "metadata": {},
   "outputs": [],
   "source": [
    "duplicates = np.full(arr.shape, True)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "e524cb80-8ef3-4171-a5d3-27b9d9389a89",
   "metadata": {},
   "outputs": [],
   "source": [
